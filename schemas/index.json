{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "$id": "https://app-image-index.netlify.app/schemas/index.json",
  "title": "Index",
  "description": "AppImageIndex repository index",
  "type": "object",
  "properties": {
    "name": {
      "description": "Repo name (kebab-case)",
      "type": "string"
    },
    "updatedAt": {
      "description": "Timestamp of last update  (ISO)",
      "type": "string"
    }
  },
  "required": ["name", "updatedAt"]
}
